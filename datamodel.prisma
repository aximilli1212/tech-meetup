 type Meetup {
      id: ID! @id
      organizer: User @relation(link:INLINE,name: "UserEvents")
      title: String!
      description: String!
      location: String!
      date: DateTime!
      attendees: [User] @relation(name: "EventAttendees")
    }
    type User {
      id: ID! @id
      email: String! @unique
      password: String!
      name: String!
      myMeetups: [Meetup!]! @relation(name: "UserEvents")
      meetupsAttending: [Meetup!]! @relation(link:INLINE,name: "EventAttendees")
    }
